    <style name="Theme.EVChargingMobile" parent="Theme.MaterialComponents.DayNight">
        
        <item name="colorPrimary">@color/primary_color</item>
        <item name="colorPrimaryVariant">@color/primary_dark</item>
        <item name="colorOnPrimary">@color/text_white</item>
        
        
        <item name="colorSecondary">@color/secondary_color</item>
        <item name="colorSecondaryVariant">@color/secondary_dark</item>
        <item name="colorOnSecondary">@color/text_white</item>
        
        
        <item name="android:colorBackground">@color/background_color</item>
        <item name="colorSurface">@color/surface_color</item>
        <item name="colorOnSurface">@color/text_primary</item>
        
        
        <item name="android:statusBarColor" ns1:targetApi="l">@color/primary_dark</item>
        
        
        <item name="android:actionBarStyle">@style/CustomActionBar</item>
        
        
        <item name="android:textColorPrimary">@color/text_primary</item>
        <item name="android:textColorSecondary">@color/text_secondary</item>
        
        
        <item name="colorError">@color/error_color</item>
    </style>
    <style name="Theme.EVChargingMobile.Splash" parent="Theme.EVChargingMobile">
        <item name="android:windowBackground">@color/primary_color</item>
        <item name="android:statusBarColor">@color/primary_dark</item>
        <item name="android:navigationBarColor">@color/primary_dark</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
